<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >


<web-app>

  <display-name>Archetype Created Web Application</display-name>
  
 
  <servlet>
  	<servlet-name>PaginationServlet1</servlet-name>
  	<display-name>PaginationServlet1</display-name>
  	<description></description>
  	<servlet-class>PaginationServlet.PaginationServlet1</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>PaginationServlet2</servlet-name>
  	<display-name>PaginationServlet2</display-name>
  	<description></description>
  	<servlet-class>PaginationServlet.PaginationServlet2</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Download</servlet-name>
  	<display-name>Download</display-name>
  	<description></description>
  	<servlet-class>PaginationServlet.Download</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>PaginationServlet</servlet-name>
  	<display-name>PaginationServlet</display-name>
  	<description></description>
  	<servlet-class>PaginationServlet.PaginationServlet</servlet-class>
  </servlet>


        
   
  <servlet-mapping>
  	<servlet-name>PaginationServlet1</servlet-name>
  	<url-pattern>/ps</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>PaginationServlet2</servlet-name>
  	<url-pattern>/PaginationServlet2</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Download</servlet-name>
  	<url-pattern>/Download</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>PaginationServlet</servlet-name>
  	<url-pattern>/PaginationServlet</url-pattern>
  </servlet-mapping>
  
   
</web-app>

